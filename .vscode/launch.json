{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "PowerShell: Launch Current File",
        "type": "PowerShell",
        "request": "launch",
        "script": "${file}",
        "cwd": "${workspaceFolder}"
      },
      {
        "name": "PowerShell: Attach to Host Process",
        "type": "PowerShell",
        "request": "attach",
        "processId": "${command:PowerShell.GetProcessId}",
        "cwd": "${workspaceFolder}"
      }
    ]
  }
  